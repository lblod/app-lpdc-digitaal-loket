    "http://purl.org/dc/terms/description": [
      {
        "@language": "nl",
        "@value": "<p data-indentation-level=\"0\">Het afschrift ontvang je in de meeste gevallen onmiddellijk in je mailbox. Bij extra controle nodig van een ambtenaar ontvang je het afschrift binnen 2 tot 5 werkdagen in je mailbox</p>"
      },
      {
        "@language": "en",
        "@value": "<p data-indentation-level=\"0\">In most cases, you will receive the copy/excerpt immediately in your mailbox. If additional verification is required by a civil servant, you will receive the copy/excerpt in your mailbox within 2 to 5 working days.</p>"
      }
    ],
